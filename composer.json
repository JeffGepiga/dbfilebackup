{
  "name": "jeff-gepiga/dbfilebackup",
  "description": "laravel package to backup/restore files and database.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jeffrey Gepiga",
      "email": "jeffreygepiga27@gmail.com",
      "homepage": "/"
    }
  ],
  "homepage": "https://github.com/jeff-gepiga/dbfilebackup",
  "keywords": [
    "Laravel",
    "Backup",
    "Restore",
    "Database",
    "Server"
  ],
  "require": {
    "illuminate/support": "~5|~6|~7|~8|~9|~10"
  },
  "autoload": {
    "psr-4": {
